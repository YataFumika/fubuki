<%- model_class = Parking -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @parking.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:address) %>:</strong></dt>
  <dd><%= @parking.address %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @parking.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:memo) %>:</strong></dt>
  <dd><%= @parking.memo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:photo) %>:</strong></dt>
  <dd><%= image_tag(@parking.photo_url, width: 300) %></dd>
</dl>

    <div id="map"></div>

  <div id="lat">x = </div>
  <div id="lng">Y = </div>
  <p> TEST </p>



<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              parkings_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_parking_path(@parking), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              parking_path(@parking),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

<script>
  initialize("<%=@parking.latitude %>", "<%=@parking.longitude %>");
</script>